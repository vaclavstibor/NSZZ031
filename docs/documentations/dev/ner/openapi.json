{
    "openapi":"3.0.1",
    "info":{
        "title":"FastAPI",
        "version":"0.1.0"
    },
    "paths":{
        "/extract-entities":{
            "post":{
                "summary":"Extract",
                "description":"Asynchronously extract entities from the input query.\n\nThis function takes in a Query object, extracts entities from the query content,\nand returns a Response object containing the list of extracted entities.\n\nArgs:\n    query (Query): The input query.\n    model (Model, optional): The model used for entity extraction. Defaults to the model in the application state.\n    connector (SPARQLWikidataConnector, optional): The connector used for entity extraction. Defaults to the connector in the application state.\n\nReturns:\n    Response: A Response object containing the list of extracted entities.",
                "operationId":"extract_extract_entities_post",
                "requestBody":{
                    "content":{
                        "application/json":{
                            "schema":{
                                "$ref":"#/components/schemas/Query"
                            }
                        }
                    },
                    "required":true
                },
                "responses":{
                    "200":{
                        "description":"Successful Response",
                        "content":{
                            "application/json":{
                                "schema":{
                                    "$ref":"#/components/schemas/Response"
                                }
                            }
                        }
                    },
                    "422":{
                        "description":"Validation Error",
                        "content":{
                            "application/json":{
                                "schema":{
                                    "$ref":"#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components":{
        "schemas":{
            "Entity":{
                "properties":{
                    "text":{
                        "type":"string",
                        "title":"Text"
                    },
                    "ticker":{
                        "type":"string",
                        "title":"Ticker"
                    }
                },
                "type":"object",
                "required":[
                    "text",
                    "ticker"
                ],
                "title":"Entity",
                "description":"A class used to represent the Entity model.\n\nAttributes:\n    text (str): The text of the entity.\n    ticker (str): The ticker of the entity."
            },
            "HTTPValidationError":{
                "properties":{
                    "detail":{
                        "items":{
                            "$ref":"#/components/schemas/ValidationError"
                        },
                        "type":"array",
                        "title":"Detail"
                    }
                },
                "type":"object",
                "title":"HTTPValidationError"
            },
            "Query":{
                "properties":{
                    "content":{
                        "type":"string",
                        "title":"Content"
                    }
                },
                "type":"object",
                "required":[
                    "content"
                ],
                "title":"Query",
                "description":"A class used to represent the Query model.\n\nAttributes:\n    content (str): The (article's) content of the query."
            },
            "Response":{
                "properties":{
                    "entities":{
                        "items":{
                            "$ref":"#/components/schemas/Entity"
                        },
                        "type":"array",
                        "title":"Entities"
                    }
                },
                "type":"object",
                "required":[
                    "entities"
                ],
                "title":"Response",
                "description":"A class used to represent the Response model.\n\nAttributes:\n    entities (List[Entity]): A list of Entity objects."
            },
            "ValidationError":{
                "properties":{
                    "loc":{
                        "items":{
                            "anyOf":[
                                {
                                    "type":"string"
                                },
                                {
                                    "type":"integer"
                                }
                            ]
                        },
                        "type":"array",
                        "title":"Location"
                    },
                    "msg":{
                        "type":"string",
                        "title":"Message"
                    },
                    "type":{
                        "type":"string",
                        "title":"Error Type"
                    }
                },
                "type":"object",
                "required":[
                    "loc",
                    "msg",
                    "type"
                ],
                "title":"ValidationError"
            }
        }
    }
}