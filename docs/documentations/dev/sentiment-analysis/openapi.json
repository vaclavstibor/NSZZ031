{
    "openapi":"3.0.1",
    "info":{
        "title":"FastAPI",
        "version":"0.1.0"
    },
    "paths":{
        "/analyse-sentiment":{
            "post":{
                "summary":"Analyse",
                "description":" Asynchronously analyse sentiment from the input query.\n\nTThis function takes in a Query object, analyses sentiment of given entities within the content from the query,\nand returns a Response object containing the list of tickers with sentiment.\n\nArgs:\n    in_query (Query): The input query.\n    model (Model, optional): The model used for entity extraction. Defaults to the model in the application state.\n\nReturns:\n    Response: A Response object containing the list of tickers with sentiment.",
                "operationId":"analyse_analyse_sentiment_post",
                "requestBody":{
                    "content":{
                        "application/json":{
                            "schema":{
                                "$ref":"#/components/schemas/Query"
                            }
                        }
                    },
                    "required":true
                },
                "responses":{
                    "200":{
                        "description":"Successful Response",
                        "content":{
                            "application/json":{
                                "schema":{
                                    "$ref":"#/components/schemas/Response"
                                }
                            }
                        }
                    },
                    "422":{
                        "description":"Validation Error",
                        "content":{
                            "application/json":{
                                "schema":{
                                    "$ref":"#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components":{
        "schemas":{
            "Entity":{
                "properties":{
                    "text":{
                        "type":"string",
                        "title":"Text"
                    },
                    "ticker":{
                        "type":"string",
                        "title":"Ticker"
                    }
                },
                "type":"object",
                "required":[
                    "text",
                    "ticker"
                ],
                "title":"Entity",
                "description":"A class used to represent the Entity model.\n\nAttributes:\n    text (str): The text of the entity.\n    ticker (str): The ticker of the entity."
            },
            "HTTPValidationError":{
                "properties":{
                    "detail":{
                        "items":{
                            "$ref":"#/components/schemas/ValidationError"
                        },
                        "type":"array",
                        "title":"Detail"
                    }
                },
                "type":"object",
                "title":"HTTPValidationError"
            },
            "Query":{
                "properties":{
                    "content":{
                        "type":"string",
                        "title":"Content"
                    },
                    "entities":{
                        "items":{
                            "$ref":"#/components/schemas/Entity"
                        },
                        "type":"array",
                        "title":"Entities"
                    }
                },
                "type":"object",
                "required":[
                    "content",
                    "entities"
                ],
                "title":"Query",
                "description":"A class used to represent the Query model.\n\nAttributes:\n    content (str): The content of the query.\n    entities (List[Entity]): A list of entities in content."
            },
            "Response":{
                "properties":{
                    "tickers":{
                        "items":{
                            "$ref":"#/components/schemas/TickerWithSentiment"
                        },
                        "type":"array",
                        "title":"Tickers"
                    }
                },
                "type":"object",
                "required":[
                    "tickers"
                ],
                "title":"Response",
                "description":"A class used to represent the Response model.\n\nAttributes:\n    tickers (List[TickerWithSentiment]): A list of tickers with sentiment."
            },
            "Sentiment":{
                "properties":{
                    "classification":{
                        "$ref":"#/components/schemas/SentimentClassification"
                    },
                    "positive":{
                        "type":"number",
                        "title":"Positive"
                    },
                    "negative":{
                        "type":"number",
                        "title":"Negative"
                    },
                    "neutral":{
                        "type":"number",
                        "title":"Neutral"
                    }
                },
                "type":"object",
                "required":[
                    "classification",
                    "positive",
                    "negative",
                    "neutral"
                ],
                "title":"Sentiment"
            },
            "SentimentClassification":{
                "type":"string",
                "enum":[
                    "NEUTRAL",
                    "POSITIVE",
                    "NEGATIVE"
                ],
                "title":"SentimentClassification"
            },
            "TickerWithSentiment":{
                "properties":{
                    "ticker":{
                        "type":"string",
                        "title":"Ticker"
                    },
                    "sentiment":{
                        "$ref":"#/components/schemas/Sentiment"
                    }
                },
                "type":"object",
                "required":[
                    "ticker",
                    "sentiment"
                ],
                "title":"TickerWithSentiment",
                "description":"A class used to represent the TickerWithSentiment model containing its ticker the sentiment of the ticker."
            },
            "ValidationError":{
                "properties":{
                    "loc":{
                        "items":{
                            "anyOf":[
                                {
                                    "type":"string"
                                },
                                {
                                    "type":"integer"
                                }
                            ]
                        },
                        "type":"array",
                        "title":"Location"
                    },
                    "msg":{
                        "type":"string",
                        "title":"Message"
                    },
                    "type":{
                        "type":"string",
                        "title":"Error Type"
                    }
                },
                "type":"object",
                "required":[
                    "loc",
                    "msg",
                    "type"
                ],
                "title":"ValidationError"
            }
        }
    }
}